{
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "client/build"
        }
      }
    ],
    "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    }
  ],
   "rewrites": [{ "source": "/(.*)", "destination": "/" }]
   
  }